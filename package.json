{
  "name": "@serggrey1992/swagger-fetch",
  "version": "1.0.0",
  "description": "Simple CLI tool for downloading Swagger/OpenAPI documentation",
  "author": {
    "name": "Sergey Grey",
    "email": "sergdiag19@gmail.com",
    "url": "https://github.com/SergGrey1992"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergGrey1992/swagger-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/SergGrey1992/swagger-fetch/issues"
  },
  "homepage": "https://github.com/SergGrey1992/swagger-fetch#readme",
  "keywords": [
    "swagger",
    "openapi",
    "api",
    "documentation",
    "download",
    "cli",
    "typescript",
    "rest",
    "api-docs"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "swagger-fetch": "./bin/swagger-fetch.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "ora": "^5.4.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}